in:
    type: file
    path_prefix: {{ env.path_prefix }}
out:
    type: td
    apikey: {{ env.apikey }}
    endpoint: {% if env.endpoint %}{{ env.endpoint }}{% else %} api.treasuredata.com{% endif %}
    database: {{ env.dbname }}
    table: {{ env.table }}
    mode: {{ env.mode }}
    default_timestamp_format: '%Y-%m-%d %H:%M:%S'
    {{ env.http_proxy }}
